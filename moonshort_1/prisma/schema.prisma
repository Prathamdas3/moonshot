generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id           Int      @id @default(autoincrement())
    name         String   @unique
    email        String   @unique
    password     String
    refreshtoken String?
    createdAt    DateTime @default(now())
    updatedAt    DateTime @updatedAt
    itmes        Items[]
}

model Items {
    id     Int     @id @default(autoincrement())
    name   String
    check  Boolean
    User   User?   @relation(fields: [userId], references: [id])
    userId Int?
}
